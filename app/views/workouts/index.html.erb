<h1>Monday day 1</h1>

<table>
  <tr>
    <th>Excercise Plan</th>
    <th>Push-Pull</th>
    <th>Complete</th>
  </tr>
  <% @workouts.each do |workout| %>
    <tr>
      <td><%= workout.name %></td>
      <td><%= workout.typing_system %></td>
      <td><%= workout.memory_management %></td>
      <td><%= button_to "delete", { action: "destroy", id: workout.id},
      {method: :delete, data:{confirm: "Are you sure?"}}
      %></td>
    </tr>
</table>
  <% end %>
